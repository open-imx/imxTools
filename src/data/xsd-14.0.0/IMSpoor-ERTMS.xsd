	<xs:element name="DetectableElement">
		<xs:annotation>
			<xs:documentation>Systeem dat de positie waarin een functioneel object zich bevindt, meldt aan een CSS.</xs:documentation>
			<xs:documentation xml:lang="NL">Detectable Element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tBaseObject">
					<xs:sequence>
						<xs:element name="RailConnectionPosition" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Logische positie van de DEC op de spoortak.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="railConnectionRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>PUIC-verwijzing naar de functionele spoortak waarop dit object wordt geprojecteerd.</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>RailConnection</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>								
								<xs:attribute name="atMeasure" type="xs:double" use="required">
									<xs:annotation>
										<xs:documentation>De positie van het event op het lijnstuk. Het aantal meters vanaf het begin van de lijn.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="objectRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>Een puic-verwijzing naar het object dat als Detectable Element fungeert.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>Signal</ObjectTypeRef>
								<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
								<ObjectTypeRef>AfSection</ObjectTypeRef>
								<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
								<ObjectTypeRef>PowerOffRelay</ObjectTypeRef>
								<ObjectTypeRef>SwitchMechanism</ObjectTypeRef>
								<ObjectTypeRef>Lock</ObjectTypeRef>
								<ObjectTypeRef>LevelCrossing</ObjectTypeRef>
								<ObjectTypeRef>BridgeControlSystem</ObjectTypeRef>
								<ObjectTypeRef>OpenLine</ObjectTypeRef>
								<ObjectTypeRef>WorkzoneSwitch</ObjectTypeRef>
								<ObjectTypeRef>ControllerCabinet</ObjectTypeRef>
								<ObjectTypeRef>KeyLock</ObjectTypeRef>
								<ObjectTypeRef>KeyLockControl</ObjectTypeRef>
								<ObjectTypeRef>TemporaryShuntingRelease</ObjectTypeRef>
								<ObjectTypeRef>BellControl</ObjectTypeRef>
								<ObjectTypeRef>TechnicalSystem</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="reportDurationType" type="tReportDurationTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>De duurindicatie die aan TCS wordt gemeld indien het element zich niet in positie bevindt.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="detectableAndLockableType" type="tDetectableAndLockableEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type Detectable And Lockable element.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
