	<xs:element name="ObjectControllerCabinet">
		<xs:annotation>
			<xs:documentation>OCK: Kast bestemd voor het onderbrengen van Object Controllers en Assentelunits.</xs:documentation>
			<xs:documentation xml:lang="NL">Object Controller Kast</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element name="ControlledObjectRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst met Infra-Objecten die door de Object-Controller worden aangestuurd.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>*</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element ref="CabinetMonitoringUnit" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="interlockingUnitRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de IXL waar de Object-Controller door wordt aangestuurd.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>InterlockingUnit</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
