	<xs:complexType name="tTrackCondition" abstract="true">
		<xs:annotation>
			<xs:documentation>Basis type voor het beschrijven van een TrackCondition voor ERTMS</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tPointObject">
				<xs:sequence>
