			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:sequence>
						<xs:element ref="SwitchCondition" minOccurs="0" maxOccurs="unbounded"/>
						<xs:choice>
							<xs:element ref="LevelTransition" minOccurs="0"/>
							<xs:element ref="ModeTransition" minOccurs="0"/>
							<xs:element ref="TsiCcsTransition" minOccurs="0"/>
							<xs:element ref="RbcRbcTransition" minOccurs="0"/>
						</xs:choice>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TransitionMarkers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TransitionMarker" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TsiCcsTransition">
