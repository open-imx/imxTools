<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.prorail.nl/IMSpoor" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace" targetNamespace="http://www.prorail.nl/IMSpoor" elementFormDefault="qualified" attributeFormDefault="unqualified" version="14.0.0">
	<xs:include schemaLocation="IMSpoor-BasicTypes.xsd"/>
	<xs:include schemaLocation="IMSpoor-TrackAssets.xsd"/>
	<xs:element name="Groupings">
		<xs:annotation>
			<xs:documentation>Gebieden of objecten welke zijn opgebouwd uit andere (Infra)objecten.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TrackAggregations" minOccurs="0"/>
				<xs:element ref="Demarcations" minOccurs="0"/>
				<xs:element ref="KilometerRibbons" minOccurs="0"/>
				<xs:element ref="SignalAspectRelations" minOccurs="0"/>
				<xs:element ref="DimmableSignalGroups" minOccurs="0"/>
				<xs:element ref="StopConnectionConfigurations" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TrackAggregations">
		<xs:annotation>
			<xs:documentation>Definities van de sporen, namelijk PPLG-sporen en Dienstregelpuntsporen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PpcTracks" minOccurs="0"/>
				<xs:element ref="FunctionalRoutes" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DemarcationMarker">
		<xs:annotation>
			<xs:documentation>Een grenspunt. Een grenspunt is een markering op de topologie wat niet te herleiden is tot een (functioneel)object.</xs:documentation>
			<xs:documentation xml:lang="NL">Grenspunt</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="railConnectionRef" type="tUUIDRef" use="required">
				<xs:annotation>
					<xs:documentation>PUIC-verwijzing naar de functionele spoortak waarop dit object wordt geprojecteerd.</xs:documentation>
					<xs:appinfo>
						<ObjectTypeRef>RailConnection</ObjectTypeRef>
					</xs:appinfo>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
				<xs:annotation>
					<xs:documentation>De richting van het event ten opzichte van de normaalrichting van de functionele-spoortak.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="atMeasure" type="xs:double">
				<xs:annotation>
					<xs:documentation>De positie van het event op het lijnstuk. Het aantal meters vanaf het begin van de lijn.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="tDemarcationMarkers" abstract="false">
		<xs:annotation>
			<xs:documentation>Een lijst van grenspunten</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="DemarcationMarker" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tFragmentsObject" abstract="true">
		<xs:complexContent>
			<xs:extension base="tBaseObject">
				<xs:sequence>
					<xs:element name="TrackFragments" type="tTrackFragments" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Een lijst van Spoortak-fragmenten.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tTrackAggregation" abstract="true">
		<xs:annotation>
			<xs:documentation>Een TrackAggregation is een samenstel van (stukken) spoortakken die worden verondersteld 'kop-staart' gekoppeld te kunnen worden en dan samen een lineair stuk spoor beschrijven. De geometrie bij dit type is een generatie-product en is niet bedoeld om aangepast te worden.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tFragmentsObject">
				<xs:sequence>
					<xs:element name="DemarcationMarkers" type="tDemarcationMarkers">
						<xs:annotation>
							<xs:documentation>Een lijst van grenspunten</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="TunnelEntryRoute">
		<xs:annotation>
			<xs:documentation>Configuratie als de Functionele route een tunnel entryroute betreft.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AdjoiningTunnelTubeRefs" type="tUUIDRefList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Een lijst van PUIC verwijzingen naar neventunnelbuizen die van invloed zijn op de routesetting.</xs:documentation>
						<xs:appinfo>
							<ObjectTypeRef>TunnelTube</ObjectTypeRef>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RouteReportingElement" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="tunnelTubeRef" type="tUUIDRef" use="required">
				<xs:annotation>
					<xs:documentation>Een PUIC verwijzing naar de bijbehorende tunnelbuis.</xs:documentation>
					<xs:appinfo>
						<ObjectTypeRef>TunnelTube</ObjectTypeRef>
					</xs:appinfo>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ttiTimer" type="tTimerValue" use="required">
				<xs:annotation>
					<xs:documentation>De vertragingstijd van de stuuropdracht in seconden</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RouteReportingElement">
		<xs:annotation>
			<xs:documentation>Het object waarmee de statusmeldingen over de tunnel per ingaande route worden gemeld aan het CSS.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="BeginSignalRef" type="tUUIDRef">
					<xs:annotation>
						<xs:documentation>Een PUIC verwijzing naar het beginsein van de functionele-Route.</xs:documentation>
						<xs:appinfo>
							<ObjectTypeRef>Signal</ObjectTypeRef>
							<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="EndSignalRef" type="tUUIDRef">
					<xs:annotation>
						<xs:documentation>Een PUIC verwijzing naar het eindsein van de functionele-Route.</xs:documentation>
						<xs:appinfo>
							<ObjectTypeRef>Signal</ObjectTypeRef>
							<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="SectionRef" type="tUUIDRef">
					<xs:annotation>
						<xs:documentation>Een PUIC verwijzing naar een sectie in de functionele-route.</xs:documentation>
						<xs:appinfo>
							<ObjectTypeRef>Signal</ObjectTypeRef>
							<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="SwitchPosition">
					<xs:annotation>
						<xs:documentation>Het wissel inclusief stand.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="tSwitchMechanismPosition"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="FunctionalRoutes">
		<xs:annotation>
			<xs:documentation>Een lijst van Treinbeveiliging routes</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FunctionalRoute" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FunctionalRoute">
		<xs:annotation>
			<xs:documentation>Voor baan- en treinbeveiliging gebruikte route met spoortak fragmenten waarin zich geen bediende lichtseinen bevinden.</xs:documentation>
			<xs:documentation xml:lang="NL">Functionele Route</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tRoute">
					<xs:sequence>
						<xs:element ref="StopSignalPassageDetection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Aanvullende beveiliging op STS-passage van seinen. Mogelijke objecten zijn Signal, Sign en StopMarkerBoard</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TrainlengthDependentDepartureSignalActivation" minOccurs="0">
							<xs:annotation>
								<xs:documentation>TAV-functie: Een beveiligingsfunctie die zorgt dat bij het begin van een route bij een vertreklicht de trein met volledige lengte het perron kan vrijrijden</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="platformClearancePointRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>Een PUIC verwijzing naar een perron-vrijrijdpunt. Alleen van toepassing bij Treinlengte Afhankelijke Vertrekseinlicht (TAV) aansturing.</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>PlatformClearancePoint</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="SectionOccupationDetectionRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar secties welke uitgelezen moeten worden ten behoeve van flankzone bewaking</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
									<ObjectTypeRef>AfSection</ObjectTypeRef>
									<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="WarningInstallationRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een PUIC verwijzing naar een werkplekbeveiligingwaarschuwingsinstallatie die door het instellen van deze route wordt geactiveerd.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>BridgeWarningInstallation</ObjectTypeRef>
									<ObjectTypeRef>LevelCrossingWarningInstallation</ObjectTypeRef>
									<ObjectTypeRef>SightRestrictingObjectWarningInstallation</ObjectTypeRef>
									<ObjectTypeRef>TunnelWarningInstallation</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element ref="TunnelEntryRoute" minOccurs="0"/>
						<xs:element name="ConditionalRouteRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst PUIC verwijzingen naar functionele-routes als vervolgroutes. Het is noodzakelijk dat de vervolgroutes zijn ingesteld voor het instellen van de huidige route.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>FunctionalRoute</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="startObjectRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>De PUIC verwijzing naar het beginobject van een Functionele-route. Dit object kan een Bord, Stop Marker Board of Lichtsein zijn</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>Signal</ObjectTypeRef>
								<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								<ObjectTypeRef>Sign</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="endObjectRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>De PUIC verwijzing naar het eindobject van een Functionele-route. (Bord, Stop Marker Board, Lichtsein of Stootjuk)</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>Signal</ObjectTypeRef>
								<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								<ObjectTypeRef>Sign</ObjectTypeRef>
								<ObjectTypeRef>BufferStop</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="tunnelTubeRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de tunnelbuis waar de route toegang tot geeft.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>TunnelTube</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="releaseDelay" type="tTimerValue">
						<xs:annotation>
							<xs:documentation>Vertragingstijd voor het handmatige restrijwegvrijmaking. In seconden</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="isReleasedAutomatically" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of de route automatisch restrijwegvrijmaking heeft.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="controllerCabinetRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar een medewerkerskastje welke bedienbaar wordt bij het instellen van de route.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>ControllerCabinet</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="hasOverheadLineSwitchMechanism" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Een indicatie of er een bovenleidingssteller in de route is opgenomen en moet worden aangestuurd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="routeTimerValue" type="tTimerValue">
						<xs:annotation>
							<xs:documentation>De waarde van de herroeptimer/routetimer vanaf het begin(sein) van de route in seconden.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="hasCargoCriteriumOption" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Geeft aan of bij de rijweg een goederen criterium kan worden opgegeven.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="hasNormalOption" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Een indicatie of de route normaal instelbaar is.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="hasDriveOnSightOption" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Een indicatie of een route met ROZ instelbaar is.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="tRoute" abstract="true">
		<xs:complexContent>
			<xs:extension base="tFragmentsObject">
				<xs:sequence>
					<xs:element ref="OrderedSwitchPosition" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>De lijst van stellerstanden voor de route.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="StopSignalPassageDetection">
		<xs:annotation>
			<xs:documentation>Aanvullende beveiliging op STS-passage van seinen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="signalRef" type="tUUIDRef" use="required">
				<xs:annotation>
					<xs:documentation>Een PUIC verwijzing naar een sein</xs:documentation>
					<xs:appinfo>
						<ObjectTypeRef>Signal</ObjectTypeRef>
						<ObjectTypeRef>Sign</ObjectTypeRef>
						<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
					</xs:appinfo>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PpcTracks">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PpcTrack" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PpcTrack">
		<xs:annotation>
			<xs:documentation>Een spoor is een ononderbroken, benaamd deel van één of meer spoortakken, eenduidig begrensd door daarvoor geldige spoorgrenzen en bedoeld als van- of naar-locatie voor een beweging, danwel als opstelplaats voor materieel. Primary Process Control (PPC = PPLG)</xs:documentation>
			<xs:documentation xml:lang="NL">Stuurspoor</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tObjectDemarcation">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="2" maxOccurs="2">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>PUIC verwijzing naar een begrenzende object</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>Signal</ObjectTypeRef>
											<ObjectTypeRef>Sign</ObjectTypeRef>
											<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
											<ObjectTypeRef>FlankProtectionSystem</ObjectTypeRef>
											<ObjectTypeRef>Crossing</ObjectTypeRef>
											<ObjectTypeRef>SingleSwitch</ObjectTypeRef>
											<ObjectTypeRef>DoubleDiamondCrossing</ObjectTypeRef>
											<ObjectTypeRef>SingleDiamondCrossing</ObjectTypeRef>
											<ObjectTypeRef>BufferStop</ObjectTypeRef>
											<ObjectTypeRef>TerraIncognita</ObjectTypeRef>
											<ObjectTypeRef>Blocking</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="KeyLockRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar sleutelkastjes voor het lokaal bedienen van het spoor.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>KeyLock</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="StopConnectionRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar stopdoor configuratie bij het spoor</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>StopConnection</ObjectTypeRef>
									<ObjectTypeRef>OpenLineStopConnection</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Demarcations">
		<xs:annotation>
			<xs:documentation>Verzameling van groepen demarcaties en demarcatie-aggregaties.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TrackVacancyDetection" minOccurs="0"/>
				<xs:element ref="NonStoppingAreas" minOccurs="0"/>
				<xs:element ref="NonCentrallyOperatedAreas" minOccurs="0"/>
				<xs:element ref="TemporaryShuntingAreas" minOccurs="0"/>
				<xs:element ref="Workzones" minOccurs="0"/>
				<xs:element ref="WorkzoneSystems" minOccurs="0"/>
				<xs:element ref="SlopeRegimeAreas" minOccurs="0"/>
				<xs:element ref="TunnelRegimeAreas" minOccurs="0"/>
				<xs:element ref="SectionBreakProjections" minOccurs="0"/>
				<xs:element ref="AtbEgSections" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AtbEgSections">
		<xs:annotation>
			<xs:documentation>Een lijst van ATB-EG Secties.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AtbEgSection" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SectionBreakProjections">
		<xs:annotation>
			<xs:documentation>Een lijst van spaninrichting-projecties.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SectionBreakProjection" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SectionBreakProjection">
		<xs:annotation>
			<xs:documentation>De projectie van de Spaninrichting</xs:documentation>
			<xs:documentation xml:lang="NL">Spaninrichting-projectie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointDemarcation">
					<xs:sequence>
						<xs:element name="SlopingWiresProjection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Projectie van een uitloopveld.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="DemarcationMarkers" type="tDemarcationMarkers">
										<xs:annotation>
											<xs:documentation>De grenspunten van een uitloopveld.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ParallelWiresProjection" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Projectie van een Overnameveld.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="DemarcationMarkers" type="tDemarcationMarkers">
										<xs:annotation>
											<xs:documentation>De grenspunten van een overnameveld.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="sectionBreakType" type="tSectionBreakTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type spaninrichting.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SlopeRegimeAreas">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van hellingregime gebieden.</xs:documentation>
				</xs:annotation>
				<xs:element ref="SlopeRegimeArea" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SlopeRegimeArea">
		<xs:annotation>
			<xs:documentation>Een hellingregime-gebied is een afgebakende ruimte op één of meer spoortakken op een helling waarin ten behoeve van (zware) goederentreinen en andere aangewezen treinen aanvullende maatregelen nodig zijn, om te voorkomen dat een trein die de helling opgaat strandt c.q. terugrolt.</xs:documentation>
			<xs:documentation xml:lang="NL">Hellingregime Gebied</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tObjectDemarcation">
					<xs:sequence>
						<xs:element name="EntrySignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) dat aan de voet van het hellingregime-gebied staat.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="FirstEndSignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) dat het eindsein vormt van een ingaande route hellingregime-gebied.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="LastBeginSignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) dat het beginsein vormt van een uitgaande route hellingregime-gebied.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ExitSignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) dat aan het einde van een hellingregime-gebied staat.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="lengthCriteriumExtraSection" type="xs:nonNegativeInteger">
						<xs:annotation>
							<xs:documentation>Lengtecriterum: Er dient een extra blok gereserveerd te worden als een trein langer is dan het lengtecriterium. Lengte in meters.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TunnelRegimeAreas">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van tunnelregime gebieden.</xs:documentation>
				</xs:annotation>
				<xs:element ref="TunnelRegimeArea" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TunnelRegimeArea">
		<xs:annotation>
			<xs:documentation>Een tunnelregime-gebied is een ruimte op één of meer spoortakken in een tunnel(buis) waarin voorkomen moet worden dat een trein die gevaarlijke stoffen vervoert zich tegelijkertijd nog andere trein(en) in het gebied te bevinden, dit om het effect van een eventuele calamiteit met de gevaarlijke stoffen in te perken.</xs:documentation>
			<xs:documentation xml:lang="NL">TunnelGebied</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tObjectDemarcation">
					<xs:sequence>
						<xs:element name="EntrySignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een route waarmee een tunnelregime-gebied ingereden wordt</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="FirstEndSignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) dat het eindsein vormt van een ingaande route tunnelregime-gebied.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="LastBeginSignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) dat het beginsein vormt van een uitgaande route van het tunnelregime-gebied.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ExitSignalRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC verwijzingen naar een Sein (SMB of een bediend hoofdsein) vanaf waar na passage het tunnelregime niet langer van kracht is.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TrackVacancyDetection">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TrackCircuit" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AfSection" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AxleCounterSection" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="SweepCorridor" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PowerOffRelay" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NonStoppingAreas">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van Doorrijdgebieden</xs:documentation>
				</xs:annotation>
				<xs:element ref="NonStoppingArea" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NonStoppingArea">
		<xs:annotation>
			<xs:documentation>Doorrijdgebied. Een gebied waarbinnen een trein niet mag stoppen. Ten behoeve van conventionele beveiliging</xs:documentation>
			<xs:documentation xml:lang="NL">Doorrijdgebied</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointDemarcation"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NonCentrallyOperatedAreas">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van Niet Centraal Bediende Gebieden</xs:documentation>
				</xs:annotation>
				<xs:element ref="NonCentrallyOperatedArea" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NonCentrallyOperatedArea">
		<xs:annotation>
			<xs:documentation>Een niet centraal bediend gebied (NCBG) is een gebied op het spoorwegnet, waarbinnen de bediening van individuele infraobjecten en de rijweginstelling lokaalplaatsvindt onder de supervisie van de treindienstleider met minimale bevoegdheid.</xs:documentation>
			<xs:documentation xml:lang="NL">Niet Centraal Bediend Gebied</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tObjectDemarcation">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>Een PUIC verwijzing naar een begrenzend object (Stopmarkerboard, Stopbord, Facultatief Stopbord, Bord einde beveiliging of Opdrachtbord)</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
											<ObjectTypeRef>Sign</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="cardinalDirection" type="tCardinalDirectionEnum" use="required">
						<xs:annotation>
							<xs:documentation>De ontworpen richting van het gebied ter ondersteuning van identificatie van toegangsroutes.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TemporaryShuntingAreas">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van Vrijgave rangeergebieden</xs:documentation>
				</xs:annotation>
				<xs:element ref="TemporaryShuntingArea" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TemporaryShuntingArea">
		<xs:annotation>
			<xs:documentation>Vrijgave rangeergebied</xs:documentation>
			<xs:documentation xml:lang="NL">Vrijgave rangeergebied</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tObjectDemarcation">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>Een PUIC verwijzing naar begrenzend object (Stopmarkerboard, Lichtsein, Bord, Ontspoorinrichting of Stootjuk)</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
											<ObjectTypeRef>Signal</ObjectTypeRef>
											<ObjectTypeRef>Sign</ObjectTypeRef>
											<ObjectTypeRef>FlankProtectionSystem</ObjectTypeRef>
											<ObjectTypeRef>BufferStop</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="LockRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst PUIC verwijzingen naar grendels welke gepaard gaan met het nemen van het Vrijgave RangeerGebied.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Lock</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element ref="DetectableElementCondition" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="PassableDemarcationRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC relaties naar de begrenzende objecten welke gepasseerd mogen worden wanneer het rangeergebied genomen is (Stopmarkerboard,Lichtsein)</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
									<ObjectTypeRef>Signal</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element ref="MandatoryFlankProtection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Een of meer eis-wissels voor het in of uitrijden van het gebied</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BridgeFloodBarrierRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar 1 of meerdere beweegbare bruggen en/of waterkeringen die voorwaardelijk zijn voor het bedienen van het vrijgave rangeergebied.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Bridge</ObjectTypeRef>
									<ObjectTypeRef>FloodBarrier</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ConditionalSectionRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar 1 of meer secties welke voorwaardelijk zijn voor het bedienen van het vrijgave rangeergebied. Deze secties moeten onbezet zijn, en kunnen binnen of buiten het VR-gebied liggen.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
									<ObjectTypeRef>AfSection</ObjectTypeRef>
									<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="LevelCrossingRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar beveiligde overwegen die geactiveerd worden wanneer het rangeergebied genomen is.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>LevelCrossing</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="WarningInstallationRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst van PUIC relaties naar waarschuwingsinstallaties (WIBR, WUBO, WIT, WIDO) die geactiveerd moeten worden wanneer het rangeergebied genomen is.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>BridgeWarningInstallation</ObjectTypeRef>
									<ObjectTypeRef>LevelCrossingWarningInstallation</ObjectTypeRef>
									<ObjectTypeRef>SightRestrictingObjectWarningInstallation</ObjectTypeRef>
									<ObjectTypeRef>TunnelWarningInstallation</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Workzones">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van werkzones</xs:documentation>
				</xs:annotation>
				<xs:element ref="Workzone" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Workzone">
		<xs:annotation>
			<xs:documentation>Een Werkzone is een vooraf gedefinieerd gebied op de topologie van de Railinfrastructuur dat in nauwe samenhang met de Beveiliging is afgebakend ter ondersteuning van het Veilig Werken aan de Infrastructuur.</xs:documentation>
			<xs:documentation xml:lang="NL">Werkzone</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tObjectDemarcation">
					<xs:sequence>
						<xs:element name="SectionRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Lijst van PUIC verwijzingen naar secties.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
									<ObjectTypeRef>AfSection</ObjectTypeRef>
									<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="workzoneSystemRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar het werkzone-systeem</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>WorkzoneSystem</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DetectableElementCondition">
		<xs:annotation>
			<xs:documentation>Een (Lockable And) Detectable Element in een bepaalde toestand als conditie.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ReportingElement" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Een verwijzing naar een element waarop gesignaleerd moet worden aan TCS.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="BeginSignalRef" type="tUUIDRef">
								<xs:annotation>
									<xs:documentation>Een PUIC verwijzing naar het beginsein van de functionele-Route.</xs:documentation>
									<xs:appinfo>
										<ObjectTypeRef>Signal</ObjectTypeRef>
										<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="EndSignalRef" type="tUUIDRef">
								<xs:annotation>
									<xs:documentation>Een PUIC verwijzing naar het eindsein van de functionele-Route.</xs:documentation>
									<xs:appinfo>
										<ObjectTypeRef>Signal</ObjectTypeRef>
										<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="SectionRef" type="tUUIDRef">
								<xs:annotation>
									<xs:documentation>Een PUIC verwijzing naar een sectie in de functionele-route.</xs:documentation>
									<xs:appinfo>
										<ObjectTypeRef>Signal</ObjectTypeRef>
										<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="SwitchPosition">
								<xs:annotation>
									<xs:documentation>Het wissel inclusief stand.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="tSwitchMechanismPosition"/>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="LevelCrossingRef" type="tUUIDRef">
								<xs:annotation>
									<xs:documentation>Een PUIC verwijzing naar een overweg.</xs:documentation>
									<xs:appinfo>
										<ObjectTypeRef>LevelCrossing</ObjectTypeRef>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="detectableElementRef" type="tUUIDRef" use="required">
				<xs:annotation>
					<xs:documentation>Een verwijzing naar een DetectableElement waarover gesignaleerd moet worden aan TCS.</xs:documentation>
					<xs:appinfo>
						<ObjectTypeRef>DetectableElement</ObjectTypeRef>
					</xs:appinfo>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="WorkzoneSystems">
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Een lijst van werkzone-systemen</xs:documentation>
				</xs:annotation>
				<xs:element ref="WorkzoneSystem" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WorkzoneSystem">
		<xs:annotation>
			<xs:documentation>Werkzone-Systeem is een geheel van verantwoordelijkheden en maatregelen (o.a. werkplekbeveiligingsinstructie (WBIs)) dat waarborgt dat er binnen een Werkzone veilig gewerkt kan worden.</xs:documentation>
			<xs:documentation xml:lang="NL">Werkzone-Systeem</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tBaseObject">
					<xs:sequence>
						<xs:element ref="MandatoryFlankProtection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>De lijst van stellerstanden als flankbeveiliging voor de werkzone.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LockRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst PUIC verwijzingen naar grendels welke gepaard gaan met het nemen van de werkzone.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Lock</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element ref="DetectableElementCondition" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="controlDelay" type="xs:int">
						<xs:annotation>
							<xs:documentation>Tijdvertraging bij het geven en nemen van de werkzone in seconden</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="workzoneSystemControlType" type="tWorkzoneSystemControlTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>De wijze waarop het werkzone-systeem kan worden gegeven of genomen.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="tDemarcationAggregation" abstract="true">
		<xs:annotation>
			<xs:documentation>een type voor een samenstel van demarcaties</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tBaseObject">
				<xs:attribute name="description" type="tDescriptionString" use="required">
					<xs:annotation>
						<xs:documentation>Volledige uitgeschreven naam van de demarcatieverzameling.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tObjectDemarcation" abstract="true">
		<xs:annotation>
			<xs:documentation>Generiek type voor een afbakening of demarcatie. De geometrie en TrackFragments bij dit type zijn een generatie-product en zijn niet bedoeld om aangepast te worden.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tFragmentsObject">
				<xs:attribute name="description" type="tDescriptionString">
					<xs:annotation>
						<xs:documentation>Volledige uitgeschreven naam van de demarcatie.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tPointDemarcation" abstract="true">
		<xs:annotation>
			<xs:documentation>Generiek type voor een afbakening of demarcatie. De geometrie en TrackFragments bij dit type zijn een generatie-product en zijn niet bedoeld om aangepast te worden.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tObjectDemarcation">
				<xs:sequence>
					<xs:element name="DemarcationMarkers" type="tDemarcationMarkers">
						<xs:annotation>
							<xs:documentation>De grenspunten van de demarcatie.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tAutomaticTrainControlSection" abstract="true">
		<xs:annotation>
			<xs:documentation>Basis type voor spoor met automatische treinbeïnvloeding.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tObjectDemarcation"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tTrainDetectionSection" abstract="true">
		<xs:annotation>
			<xs:documentation>Basis type voor treindetectie secties.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tObjectDemarcation">
				<xs:sequence>
					<xs:element ref="ReleaseMethod" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
				<xs:attribute name="controllerCabinetRef" type="tUUIDRef">
					<xs:annotation>
						<xs:documentation>Een PUIC verwijzing naar een medewerkerskastje (HC-kastje) welke bedienbaar wordt bij het bezet melden van de sectie.</xs:documentation>
						<xs:appinfo>
							<ObjectTypeRef>ControllerCabinet</ObjectTypeRef>
						</xs:appinfo>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="isTrapSectionSituation" type="tTrueFalseEnum" use="required">
					<xs:annotation>
						<xs:documentation>Indicatie of er een trap situatie ontstaat.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="isLampSection" type="tTrueFalseEnum" use="required">
					<xs:annotation>
						<xs:documentation>Geeft aan of de sectie een lampsectie is. Sectie waar weliswaar detectie plaatsvindt door een treinbeveiligingssysteem, maar waar de meldingen over de spoorbezetting voor die sectie niet gebruikt wordt voor enige veiligheidsfunctie binnen Treinbeveiliging en/of -beheersing. De enige functie is het informeren over de sectiebezetting aan een treindienstleider.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="cssPickUpDelayTime" type="tTimerValue">
					<xs:annotation>
						<xs:documentation>Vrijmeldvertraging in het CSS bij de statusverandering van een TVP-sectie van bezet naar onbezet. Tijd in seconden</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ReleaseMethod">
		<xs:annotation>
			<xs:documentation>Rijwegvrijmaking</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="releasePrinciple" type="tReleasePrincipleEnum" use="required">
				<xs:annotation>
					<xs:documentation>Vrijmakingsprincipe. De lijst van opties is voorgedefiniëerd.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
				<xs:annotation>
					<xs:documentation>De richting ten opzichte van de default richting van de spoortak behorende bij de sectie gewijze rijwegvrijmaking.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PowerOffRelay">
		<xs:annotation>
			<xs:documentation>Bewaakt de onderspanningsgrens van de voeding en geeft een betrouwbaarheidsmelding mbt een bezetmelding van een sectie.</xs:documentation>
			<xs:documentation xml:lang="NL">POR</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tBaseObject">
					<xs:sequence>
						<xs:element name="SectionRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst PUIC verwijzingen naar secties.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
									<ObjectTypeRef>AfSection</ObjectTypeRef>
									<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="SignalRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst PUIC verwijzingen naar seinen of SMB's.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="porType" type="tPorTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Type POR-melding voor de sectie.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="housingRef" type="tUUIDRef" use="optional">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de behuizing.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>InstallationCabinet</ObjectTypeRef>
								<ObjectTypeRef>EquipmentRoom</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PreCoding">
		<xs:annotation>
			<xs:documentation>Voorcodering is een functionaliteit waarbij het versturen van de ATB-code niet geïnitieerd wordt door de detectie van een eigen sectiebezetting sectie maar al door de voorliggende sectie.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Path" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Voor voorwaardelijke wisselstanden om de voorcodering te activeren.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="sectionRef" type="tUUIDRef" use="required">
				<xs:annotation>
					<xs:documentation>Puic-verwijzing naar de sectie die de voorcodering initieert.</xs:documentation>
					<xs:appinfo>
						<ObjectTypeRef>AtbEgSection</ObjectTypeRef>
						<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
					</xs:appinfo>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AtbEgSection">
		<xs:annotation>
			<xs:documentation>Begrensde ruimte op het spoor waarbinnen een ATB-code afgegeven wordt. Deze secties worden niet gebruikt voor treindetectie.</xs:documentation>
			<xs:documentation xml:lang="NL">ATB-EG-Sectie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tAutomaticTrainControlSection">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>PUIC verwijzingen naar de begrenzende ES-Lassen</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>InsulatedJoint</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element ref="PreCoding" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TrackCircuit">
		<xs:annotation>
			<xs:documentation>Sectie waarbij detectie plaatsvindt door gebruik te maken van het feit dat een as van een trein een elektrische stroomkring (de spoorstroomloop) op een spoorweg doorbreekt.</xs:documentation>
			<xs:documentation xml:lang="NL">Spoorstroomloop-Sectie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tTrainDetectionSection">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>PUIC verwijzingen naar de begrenzende ES-Lassen</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>InsulatedJoint</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="ReferenceDirection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Sectie-met-Sectierichting is een verzameling sectiescheiders die gemeenschappelijke sectierichting hebben bij binnenkomst van de sectie via de voornoemde sectiescheiders.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="EntryBlockDemarcationRefs" type="tUUIDRefList">
										<xs:annotation>
											<xs:documentation>PUIC verwijzingen naar de gerelateerde inrijd-sectiescheider(s) welke bij de sectie-richting horen.</xs:documentation>
											<xs:appinfo>
												<ObjectTypeRef>InsulatedJoint</ObjectTypeRef>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="sectionDirection" type="tSectionDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>Een abstractie van de richting van een sectie, gegeven de richting waarmee een trein die sectie binnenrijdt</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element ref="PreCoding" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="trackCircuitType" type="tTrackCircuitTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type detectiesysteem binnen de sectie. De lijst van opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="highVoltageDetectionCabinetRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de bijbehorende PSSSL-kast</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>HighVoltageDetectionCabinet</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AfSection">
		<xs:annotation>
			<xs:documentation>Spoorstroomloop-sectie waar de frequentie van de spoorstroomloop verschilt van de aangrenzende secties.</xs:documentation>
			<xs:documentation xml:lang="NL">TF-Sectie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tTrainDetectionSection">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>PUIC verwijzingen naar de begrenzende TF-Lassen of ES-Lassen</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>AudioFrequencyJoint</ObjectTypeRef>
											<ObjectTypeRef>InsulatedJoint</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="AudioFrequencyReceiverRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst van puic referenties naar Toonfrequent-Ontvangers van de sectie</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>AfTuningUnit</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ReferenceDirection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Sectie-met-Sectierichting is een verzameling sectiescheiders die gemeenschappelijke sectierichting hebben bij binnenkomst van de sectie via de voornoemde sectiescheiders.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="EntryBlockDemarcationRefs" type="tUUIDRefList">
										<xs:annotation>
											<xs:documentation>PUIC verwijzingen naar de gerelateerde inrijd-sectiescheider(s) welke bij de sectie-richting horen.</xs:documentation>
											<xs:appinfo>
												<ObjectTypeRef>AudioFrequencyJoint</ObjectTypeRef>
												<ObjectTypeRef>InsulatedJoint</ObjectTypeRef>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="sectionDirection" type="tSectionDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>Een abstractie van de richting van een sectie, gegeven de richting waarmee een trein die sectie binnenrijdt</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="afSectionType" type="tAfSectionTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type toon-frequente detectiesysteem dat is gebruikt. De lijst van opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="audioFrequencyTransmitterRef" type="tUUIDRef" use="optional">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de bijbehorende Toonfrequent-Zender van de sectie</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>AfTuningUnit</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AxleCounterSection">
		<xs:annotation>
			<xs:documentation>Een sectie die werkt door aan het begin van een sectie en aan het eind van de sectie, het aantal gepasseerde assen te tellen. Is het totaal 0, dan zijn er evenveel assen uit- als ingereden, dan is de sectie vrij.</xs:documentation>
			<xs:documentation xml:lang="NL">Assentellersectie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tTrainDetectionSection">
					<xs:sequence>
						<xs:element name="DemarcationObject" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="objectRef" type="tUUIDRef" use="required">
									<xs:annotation>
										<xs:documentation>PUIC verwijzingen naar de begrenzende assentelpunten</xs:documentation>
										<xs:appinfo>
											<ObjectTypeRef>AxleCounterDetectionPoint</ObjectTypeRef>
										</xs:appinfo>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="direction" type="tFunctionalDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>De richting van de begrenzer ten opzichte van de normaalrichting van de functionele-spoortak. Begrenzers wijzen altijd naar de binnenkant van het gebied.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="ReferenceDirection" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Sectie-met-Sectierichting is een verzameling sectiescheiders die gemeenschappelijke sectierichting hebben bij binnenkomst van de sectie via de voornoemde sectiescheiders.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="EntryBlockDemarcationRefs" type="tUUIDRefList">
										<xs:annotation>
											<xs:documentation>PUIC verwijzingen naar de gerelateerde inrijd-sectiescheider(s) welke bij de sectie-richting horen.</xs:documentation>
											<xs:appinfo>
												<ObjectTypeRef>AxleCounterDetectionPoint</ObjectTypeRef>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="sectionDirection" type="tSectionDirectionEnum" use="required">
									<xs:annotation>
										<xs:documentation>Een abstractie van de richting van een sectie, gegeven de richting waarmee een trein die sectie binnenrijdt</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="axleCounterUnitRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de bijbehorende assentelunit</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>AxleCounterUnit</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="axleCounterType" type="tAxleCounterTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type/uitvoering van het assentellersysteem</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="axleCounterSectionFunction" type="tAxleCounterSectionFunctionEnum" use="required">
						<xs:annotation>
							<xs:documentation>De functie die het assentellersysteem vervuld</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="baliseGroupRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de balisegroep die door een veegtrein wordt gebruikt om een kopspoor als leeg te detecteren.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>BaliseGroup</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="allowResetByExternalSystem" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of de assentelsectie door het monitoringssysteem kan worden gereset.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SweepCorridor">
		<xs:annotation>
			<xs:documentation>Een serie treinbewegingen om een gestoorde assentellersectie vrij te kunnen geven.</xs:documentation>
			<xs:documentation xml:lang="NL">Veegset</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tBaseObject">
					<xs:sequence>
						<xs:element ref="SweepMovement" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="axleCounterSectionRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de te vegen assentellersectie</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
