<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.prorail.nl/IMSpoor" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.prorail.nl/IMSpoor" elementFormDefault="qualified" attributeFormDefault="unqualified" version="12.0.0">
	<xs:include schemaLocation="IMSpoor-BasicTypes.xsd"/>
	<xs:element name="Furniture">
		<xs:annotation>
			<xs:documentation>Een verzameling objecten zonder een directe relatie tot een spoortak</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tDesignPetal">
					<xs:sequence>
						<xs:element ref="NoiseBarriers" minOccurs="0"/>
						<xs:element ref="HectometerSigns" minOccurs="0"/>
						<xs:element ref="LightTowers" minOccurs="0"/>
						<xs:element ref="PassengerInfoPanels" minOccurs="0"/>
						<xs:element ref="Fences" minOccurs="0"/>
						<xs:element ref="Gates" minOccurs="0"/>
						<xs:element ref="Culverts" minOccurs="0"/>
						<xs:element ref="ServiceStations" minOccurs="0"/>
						<xs:element ref="WeatherVanes" minOccurs="0"/>
						<xs:element ref="FireHydrants" minOccurs="0"/>
						<xs:element ref="FillHydrants" minOccurs="0"/>
						<xs:element ref="TensionWireLights" minOccurs="0"/>
						<xs:element ref="AntiSuicideLightingInstallations" minOccurs="0"/>
						<xs:element ref="TerrainLightingInstallations" minOccurs="0"/>
						<xs:element ref="WildlifeGuards" minOccurs="0"/>
						<xs:element ref="DeerEscapes" minOccurs="0"/>
						<xs:element ref="BadgerTunnels" minOccurs="0"/>
						<xs:element ref="AmphibianScreens" minOccurs="0"/>
						<xs:element ref="AntiSwanProvisions" minOccurs="0"/>
						<xs:element ref="RoeTrenches" minOccurs="0"/>
						<xs:element ref="WildlifeFences" minOccurs="0"/>
						<xs:element ref="RetainingWalls" minOccurs="0"/>
						<xs:element ref="OhsFences" minOccurs="0"/>
						<xs:element ref="NonRailSigns" minOccurs="0"/>
						<xs:element ref="TransmissionTowers" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TerrainLightingInstallations">
		<xs:annotation>
			<xs:documentation>Een verzameling terreinverlichtingsinstallaties</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TerrainLightingInstallation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TerrainLightingInstallation">
		<xs:annotation>
			<xs:documentation>Terreinverlichting is het verlichtingssysteem dat bij paden en sporen wordt aangebracht ten behoeve van de oriëntatie en zichtbaarheid van spoorobjecten bij afwezigheid van daglicht.</xs:documentation>
			<xs:documentation xml:lang="NL">Terreinverlichting</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element ref="LightMast" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LightMast">
		<xs:annotation>
			<xs:documentation>Lichtmast</xs:documentation>
			<xs:documentation xml:lang="NL">Lichtmast</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AntiSuicideLightingInstallations">
		<xs:annotation>
			<xs:documentation>Een verzameling anti-suïcide verlichtingsinstallaties</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AntiSuicideLightingInstallation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AntiSuicideLightingInstallation">
		<xs:annotation>
			<xs:documentation>De anti-suïcideverlichting is verlichting parallel aan de buitenzijde van het spoor, die aangeschakeld wordt d.m.v. bewegingsdetectie als er in het donker iemand loopt.</xs:documentation>
			<xs:documentation xml:lang="NL">Anti-suïcideverlichting</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element ref="AntiSuicideLampPost" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AntiSuicideLampPost">
		<xs:annotation>
			<xs:documentation>Een paal van een anti-suïcide verlichtingsinstallatie.</xs:documentation>
			<xs:documentation xml:lang="NL">Anti-suïcideverlichtingspaal</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="WildlifeGuards">
		<xs:annotation>
			<xs:documentation>Een verzameling wildroosters</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="WildlifeGuard" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WildlifeGuard">
		<xs:annotation>
			<xs:documentation>Een samenstel van elementen die samen een voorziening vormen onder, over of naast de spoorbaan specifiek bedoelt voor de vrije doorgang, geleiding, of verblijfplaats van bepaalde diersoorten.</xs:documentation>
			<xs:documentation xml:lang="NL">Wildrooster</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeerEscapes">
		<xs:annotation>
			<xs:documentation>Een verzameling Herteninspringen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DeerEscape" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeerEscape">
		<xs:annotation>
			<xs:documentation>Een samenstel van elementen die samen een voorziening vormen onder, over of naast de spoorbaan specifiek bedoelt voor de vrije doorgang, geleiding, of verblijfplaats van bepaalde diersoorten.</xs:documentation>
			<xs:documentation xml:lang="NL">Herteninspring</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BadgerTunnels">
		<xs:annotation>
			<xs:documentation>Een verzameling dassenkleppen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BadgerTunnel" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BadgerTunnel">
		<xs:annotation>
			<xs:documentation>Een samenstel van elementen die samen een voorziening vormen onder, over of naast de spoorbaan specifiek bedoelt voor de vrije doorgang, geleiding, of verblijfplaats van bepaalde diersoorten.</xs:documentation>
			<xs:documentation xml:lang="NL">Dassenklep</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AmphibianScreens">
		<xs:annotation>
			<xs:documentation>Een verzameling Amfibieën-schermen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AmphibianScreen" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AmphibianScreen">
		<xs:annotation>
			<xs:documentation>Een samenstel van elementen die samen een voorziening vormen onder, over of naast de spoorbaan specifiek bedoelt voor de vrije doorgang, geleiding, of verblijfplaats van bepaalde diersoorten.</xs:documentation>
			<xs:documentation xml:lang="NL">Amfibieën-scherm</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AntiSwanProvisions">
		<xs:annotation>
			<xs:documentation>Een verzameling Antizwanen voorzieningen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AntiSwanProvision" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AntiSwanProvision">
		<xs:annotation>
			<xs:documentation>Een samenstel van elementen die samen een voorziening vormen onder, over of naast de spoorbaan specifiek bedoelt voor de vrije doorgang, geleiding, of verblijfplaats van bepaalde diersoorten.</xs:documentation>
			<xs:documentation xml:lang="NL">Antizwanen voorziening</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RoeTrenches">
		<xs:annotation>
			<xs:documentation>Een verzameling reegoten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RoeTrench" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RoeTrench">
		<xs:annotation>
			<xs:documentation>Voorziening voor de geleiding van reeën.</xs:documentation>
			<xs:documentation xml:lang="NL">Reegoot</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="WildlifeFences">
		<xs:annotation>
			<xs:documentation>Een verzameling wildrasters</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="WildlifeFence" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WildlifeFence">
		<xs:annotation>
			<xs:documentation>Een samenstel van elementen die samen een voorziening vormen onder, over of naast de spoorbaan specifiek bedoelt voor de vrije doorgang, geleiding, of verblijfplaats van bepaalde diersoorten.</xs:documentation>
			<xs:documentation xml:lang="NL">Wildraster</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TensionWireLights">
		<xs:annotation>
			<xs:documentation>Een verzameling SpandraadVerlichtingen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TensionWireLight" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TensionWireLight">
		<xs:annotation>
			<xs:documentation>Spandraadverlichting bestaat uit een draad/kabel met daaraan een aantal armaturen.</xs:documentation>
			<xs:documentation xml:lang="NL">Spandraadverlichting</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FillHydrants">
		<xs:annotation>
			<xs:documentation>Een verzameling bluswatervulhydranten.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FillHydrant" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FillHydrant">
		<xs:annotation>
			<xs:documentation>De bluswatervulhydrant is de voedingsaansluiting op een droge blusleiding. Van hieruit wordt bij een incident door de brandweer water in het droge blusleidingsysteem gepompt.</xs:documentation>
			<xs:documentation xml:lang="NL">Bluswatervulhydrant</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FireHydrants">
		<xs:annotation>
			<xs:documentation>Een verzameling bluswaterafnamehydranten.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FireHydrant" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FireHydrant">
		<xs:annotation>
			<xs:documentation>De bluswaterafnamehydrant is een tappunt op een onder druk staande waterleiding voor het aansluiten van brandslangen om te voorzien in bluswater.</xs:documentation>
			<xs:documentation xml:lang="NL">Bluswaterafnamehydrant</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="WeatherVanes">
		<xs:annotation>
			<xs:documentation>Een verzameling windvanen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="WeatherVane" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WeatherVane">
		<xs:annotation>
			<xs:documentation>Een windvaan of windzak is een instrument om de richting en eventueel snelheid van de wind mee te bepalen.</xs:documentation>
			<xs:documentation xml:lang="NL">Windvaan</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ServiceStations">
		<xs:annotation>
			<xs:documentation>Een verzameling servicepunten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ServiceStation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ServiceStation">
		<xs:annotation>
			<xs:documentation>Een Servicepunt is een kast met sanitaire faciliteiten en aansluitingen voor elektriciteit.</xs:documentation>
			<xs:documentation xml:lang="NL">Servicepunt</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:attribute name="electricityConnection" type="tElectricityConnectionTypeEnum" use="required"/>
					<xs:attribute name="hasCompressedAirConnection" type="tTrueFalseEnum" use="required"/>
					<xs:attribute name="waterConnection" type="tWaterConnectionTypeEnum" use="required"/>
					<xs:attribute name="hasWashFillHydrant" type="tTrueFalseEnum" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Culverts">
		<xs:annotation>
			<xs:documentation>Een verzameling duikers</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Culvert" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Culvert">
		<xs:annotation>
			<xs:documentation>Een duiker is een koker dwars onder een baan- of weglichaam door.</xs:documentation>
			<xs:documentation xml:lang="NL">Duiker</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Fences">
		<xs:annotation>
			<xs:documentation>Een verzameling hekwerken</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Fence" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Fence">
		<xs:annotation>
			<xs:documentation>Onder hekwerk verstaan we alle constructies ter afscherming van de baan.</xs:documentation>
			<xs:documentation xml:lang="NL">Hekwerk</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject">
					<xs:attribute name="height" type="xs:double">
						<xs:annotation>
							<xs:documentation>Hoogte t.o.v. Maaiveld in meters</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="fenceType" type="tFenceTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type hekwerk.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Gates">
		<xs:annotation>
			<xs:documentation>Een verzameling Toegangspoorten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Gate" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Gate">
		<xs:annotation>
			<xs:documentation>Een toegangspoort is een toegang in een hekwerk, haag of geluidsscherm.</xs:documentation>
			<xs:documentation xml:lang="NL">Toegangspoort</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element ref="FireDepartmentKeyBox" minOccurs="0" maxOccurs="1"/>
					</xs:sequence>
					<xs:attribute name="hasEscapeFunction" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Heeft de toegangspoort een vluchtfunctie.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FireDepartmentKeyBox">
		<xs:annotation>
			<xs:documentation>Brandweersleutelkluis/noodsleutelkluis is een af te sluiten behuizing om een sleutel in op te bergen t.b.v. hulpdiensten.</xs:documentation>
			<xs:documentation xml:lang="NL">Brandweersleutelkluis</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RetainingWalls">
		<xs:annotation>
			<xs:documentation>Een verzameling ballastkeringen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RetainingWall" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RetainingWall">
		<xs:annotation>
			<xs:documentation>Een grond/ballastkerende constructie.</xs:documentation>
			<xs:documentation xml:lang="NL">Ballastkering</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLineObject">
					<xs:attribute name="civilConstructionRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar een kunstwerk.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>CivilConstruction</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassengerInfoPanels">
		<xs:annotation>
			<xs:documentation>Een verzameling informatieschermen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PassengerInfoPanel" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassengerInfoPanel">
		<xs:annotation>
			<xs:documentation>Reizigersinformatiesysteem (RIS) -borden</xs:documentation>
			<xs:documentation xml:lang="NL">Reizigersinformatiesysteem (RIS)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:attribute name="infoPanelType" type="tInfoPanelEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type van het RIS-bord. De lijst voor opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NoiseBarriers">
		<xs:annotation>
			<xs:documentation>Een verzameling geluidsschermen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NoiseBarrier" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NoiseBarrier">
		<xs:annotation>
			<xs:documentation>Kunstmatige voorziening in het landschap teneinde (spoorweg)lawaai voor het achterliggende gebied te reduceren.</xs:documentation>
			<xs:documentation xml:lang="NL">Geluidsscherm</xs:documentation>
		</xs:annotation>
		<xs:complexType>
