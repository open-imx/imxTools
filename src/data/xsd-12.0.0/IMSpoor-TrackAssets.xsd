				<xs:element ref="GuardRails" minOccurs="0"/>
				<xs:element ref="ExpansionInstallations" minOccurs="0"/>
				<xs:element ref="FloodBarriers" minOccurs="0"/>
				<xs:element ref="LubricationNozzles" minOccurs="0"/>
				<xs:element ref="BridgeTransitions" minOccurs="0"/>
				<xs:element ref="MonitoringLocations" minOccurs="0"/>
