	<xs:simpleType name="tSignEnum">
		<xs:annotation>
			<xs:documentation>Enumeratie van bordtypen.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Unknown">
				<xs:annotation>
					<xs:documentation xml:lang="NL">Onbekend</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ReduceSpeedCountdownMarker">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 249a= Keperbaken, snelheid begrenzen tot 40 kilometer per uur of zoveel minder als nodig om voor het eerstvolgende ‘stop’ tonende sein te kunnen stoppen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Block">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 331= Blokbord Stop voor het bord, tenzij van de baanvakleider of treindienstleider toestemming is verkregen het bord voorbij te rijden</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EndTrainVacancyDetection">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 333= Bord einde beveiliging</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EndOverheadLine">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 311= Bord einde Elektrische Tractie</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EndOverheadLineLeft">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 311= Einde elektrische tractie bij links berijden wissel</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EndOverheadLineRight">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 311= Einde elektrische tractie bij rechts berijden wissel</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AtbCabSignalling">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 328b= ATB: cabinesignalering is leidend vanaf dit punt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EnableEtcsCabSignalling">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 336= ETCS-cabineseingeving actief. Treinen zonder ECTScabinesignalering zo spoedig mogelijk stoppen en opdracht van de treindienstleiding opvolgen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DisableEtcsCabSignalling">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 337= Einde ETCS-cabineseingeving of ETCSrijtoestemming.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Horn">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 312= Fluit</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EnableAtbCabSignalling">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 328= ATB: inschakelen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OptionalHorn">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 312a= Mistfluitbord, faculatief fluitbord</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TractionChangeover">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 320= Omschakelen naar een andere bovenleidingsspanning</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopSignCallOfficer">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 300= Stopbord: Stoppen vóór het bord. Na contact met en toestemming van de op het onderbord vermelde functionaris mag verder worden gereden. Indien het onderbord meerdere opdrachten vermeldt deze uitvoeren alvorens het bord voorbij te rijden.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopSignCallOfficerSwitchStatus">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 300a/b= Stopbord met wittelamp: Stoppen vóór het bord. Na contact met en toestemming van de op het onderbord vermelde functionaris mag verder worden gereden. Lamp aan: De wissels achter het sein liggen in de stand zoals aangegeven door het wisselsein, seinnummer 253a/b. Lamp uit: De wissels achter het sein worden lokaal bediend.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MarshallingYard">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 302= Rangeergrens aanduiding</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ProceedAfterPermission">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 301b= Facultatief stopbord: Stoppen vóór het bord, tenzij de opdracht vermeld op het onderbord, is uitgevoerd.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DriveBySight">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 317= Rijden op zicht</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopSign">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 301= 'S'-bord: Stoppen vóór het bord en de opdracht, vermeld op het onderbord, uitvoeren. Indien geen opdracht wordt vermeld, mag na het stoppen verder worden gereden.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopSignElectricTraction">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 301a= Stoppen voor het bord. Geldt alleen voor bestuurders van losse, elektrische locomotieven die niet geschikt zijn voor 15.000 V wisselspanning. Geldt alleen voor bestuurders van losse, elektrische locomotieven die niet geschikt zijn voor 1.500 V gelijkspanning.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopSignal">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 242= stopsein</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LanternStop">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 243= Afsluitlantaarn/-bord: Stoppen vóór het sein. Indien het sein op een beweegbare brug is geplaatst, stoppen voor de brug. Eventueel is het bord bij een stootjuk dubbel uitgevoerd en/of voorzien van een rode lamp.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LanternSafe">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 244a/b= Afsluitlantaarn veilig: De inrichting(en) achter het sein is veilig berijdbaar.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LowerPantograph">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 308a= Aankondiging stroomafnemers neerlaten.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LowerPantographSpecific">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 308b= stroomafnemers neer: Binnen driehonderd meter de stroomafnemers neerlaten. Geldt alleen voor bestuurders van treinen die door de betrokken spoorwegonderneming zijn aangewezen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PantographDown">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 309a= Stroomafnemers moeten zijn neergelaten.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RaisePantograph">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 310a= Stroomafnemers opzetten</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RaisePantographSpecific">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 310b= Toestemming om de stroomafnemers op te zetten. Geldt alleen voor bestuurders van treinen die door de betrokken spoorwegonderneming zijn aangewezen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TextSign">
				<xs:annotation>
					<xs:documentation xml:lang="NL">Tekstbord</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DisableAtbCabSignalling">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 329= ATB: uitschakelen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HaltAhead">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 305= Informatie voor de machinist bij het naderen van een HALTE.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WrongTrack">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 322= Bord verkeerd spoor rijden</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SwitchSign">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 253a/b= Aanduiding van de stand van het wissel. a. linksleidend, en b. rechtsleidend.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WeighBridgeSpeed">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 274= Berijden van de weegbrug toegestaan met de voor die brug geldende snelheid overeenkomstig sein ‘Nr. 324 Snelheidsbord Overweg/Weegbrug’.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WeighBridgeLocalSpeed">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 275= Berijden van de weegbrug toegestaan met de plaatselijke snelheid.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TrainLength">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 304a= Aanduiding van de plaats waar de voorzijde van een trein tot stilstand moet komen voor een goede dienstuitvoering.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TrainLengthSingle">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 304b= Aanduiding van de plaats waar de voorzijde van een trein, bestaande uit ten hoogste het aantal door het getal aangegeven spoorvoertuigen, tot stilstand moet komen voor een goede dienstuitvoering.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TrainLengthDouble">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 304c= Aanduiding van de plaats waar de voorzijde van een trein tot stilstand moet komen voor een goede dienstuitvoering. Het bovenste, witte cijfer geldt voor treinen die de wissels langs het perron in de rechte stand berijden. Het onderste, blauwe cijfer geldt voor treinen die de wissels langs het perron in de afbuigende stand berijden. Borden gelden alleen voor bestuurders van treinen bestemd voor het vervoer van personen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DisableTractionCurrent">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 306a= Uitschakelen tractiestroom.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DisableTractionCurrentSpecific">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 306b= Uitschakelbord: Uitschakelen tractiestroom. Geldt alleen voor bestuurders van treinen, voorzien van stroomafnemers, die door de betrokken spoorwegonderneming zijn aangewezen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EnableTractionCurrent">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 307a= Inschakelbord: Inschakelen tractiestroom toegestaan. Indien een onderbord is aangebracht geldt voor getrokken treinen: inschakelen tractiestroom mag slechts geschieden indien het aantal elektrische locomotieven ten hoogste het op het onderbord vermelde aantal bedraagt. Voor overige treinen: Inschakelen tractiestroom mag slechts geschieden indien de trein de op het onderbord aangegeven treinlengte niet overschrijdt.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EnableTractionCurrentSpecific">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 307b= Inschakelbord: Inschakelen tractiestroom toegestaan. Indien een onderbord is aangebracht, mag het inschakelen van de tractiestroom slechts geschieden indien de trein uit niet meer dan het aantal op het onderbord vermelde spoorvoertuigen bestaat. Geldt alleen voor bestuurders van treinen, voorzien van stroomafnemers, die door de betrokken spoorwegonderneming zijn aangewezen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LevelCrossingAnnouncement">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 318a= Aanduiding van het beginpunt van de aankondiging van een aki, ahob of aob. De cijfers op het bord geven de kilometer- en hectometeraanduiding van de spoorwegovergang aan.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DoubleLevelCrossingAnnouncement">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 318b= Aanduiding van het beginpunt van de aankondiging van twee, achter elkaar gelegen aki’s, ahob’s of aob’s. De cijfers op de borden geven de kilometer en hectometeraanduiding van de spoorwegovergangen aan, waarbij het bovenste cijfer betrekking heeft op de verst verwijderde spoorwegovergang.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StartLocalSpeedLimit">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 325a= Aanduiding van een tijdelijk verlaagde plaatselijke snelheid.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LocalSpeedLimit">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 326a= Aanduiding van het begin van een spoorgedeelte waarvoor een tijdelijke snelheidsbeperking volgens het voorafgaande tijdelijke snelheidsbord geldt.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EndLocalSpeedLimit">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 327a= Aanduiding van het einde van een spoorgedeelte waarvoor de tijdelijke snelheidsbeperking geldt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ChangeGSMR">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 338= GSM-R handmatig omschakelen naar het aangegeven nationale netwerk.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SignalingRight">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 349= Aanduiding van de plaatsing van de seinen aan de rechterzijde van het spoor waarvoor zij bestemd zijn.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BelgianSignalMarker">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 349a= Aanduiding van het spoor waarvoor het sein bestemd is. (Belgisch)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SignalingOutOfService">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 350= Aanduiding van een of meer volgende lichtseinen overeenkomstig ‘Nr. 351 Afgekruist lichtsein’ die zo mogelijk zijn gedoofd.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TrainLengthAntiicing">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 373= Aanduiding van de plaats waar een met anti-icing behandelde trein de anti-icinginstallatie heeft verlaten. Het onderbord geeft de lengte van de trein aan in rijtuigen. De trein heeft de antiicinginstallatie verlaten indien deze uit het aantal op het onderbord vermelde spoorvoertuigen bestaat.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopLocation">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 512a= Aanduiding van de plaats waar de voorzijde van een trein tot stilstand moet komen voor een goede dienstuitvoering. Geldt alleen voor bestuurders van treinen voor het vervoer van personen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopBeforeSignal">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 513= Afsluitbord: Stoppen vóór het sein.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ATBBrakeCriterium">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 804= Aanduiding van het voldoen aan het ATBremkriterium.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TrainProtectionSystemOutOfService">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 805= Aanduiding van het buiten dienst zijn van het automatisch treinbeïnvloedingssysteem.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PantographDownSpecific">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 309b= De stroomafnemers moeten zijn neergelaten. Geldt alleen voor bestuurders van treinen die door de betrokken spoorwegonderneming zijn aangewezen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PantographDownWashFacility">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 309c= De stroomafnemers moeten zijn neergelaten bij gebruik van de wasinrichting.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CommandSign">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 375= Opdrachtbord: De opdracht vermeld op het onderbord uitvoeren alvorens het bord voorbij te rijden. Dit bord wordt onder meer toegepast op locaties waarover wegverkeer plaatsvindt en de machinist een overweg moet bedienen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DecelerateLevelCrossing">
				<xs:annotation>
					<xs:documentation xml:lang="NL">RS 226a= Afrembord voor overwegen in onbeveiligd gebied</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
