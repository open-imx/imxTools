			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:attribute name="bridgeControlSystemRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar een bruginstallatie.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>BridgeControlSystem</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="floodBarrierRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar een waterkering</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>FloodBarrier</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BridgePosts">
