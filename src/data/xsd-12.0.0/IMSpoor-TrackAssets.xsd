						<xs:annotation>
							<xs:documentation>Indicatie of het kunstwerk een spoordragende functie heeft.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="civilConstructionType" type="tCivilConstructionTypeEnum" use="required">
