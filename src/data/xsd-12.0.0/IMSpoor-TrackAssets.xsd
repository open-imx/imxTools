			<xs:sequence>
				<xs:element ref="KeyLock" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="KeyLock">
		<xs:annotation>
			<xs:documentation>Sleutelkastje. Een voorziening om lokale sporen te kunnen bedienen.</xs:documentation>
			<xs:documentation xml:lang="NL">Sleutelkastje</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLocalControlEquipment">
					<xs:attribute name="firstKeyType" type="tKeyTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het sleutel type van de eerste sleutel</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="secondKeyType" type="tKeyTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het sleutel type van de optionele tweede sleutel</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
