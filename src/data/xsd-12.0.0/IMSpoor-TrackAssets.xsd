			<xs:sequence>
				<xs:element ref="LevelCrossingControl" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LevelCrossingControl">
		<xs:annotation>
			<xs:documentation>Lokaal Overweg Schakelmateriaal.</xs:documentation>
			<xs:documentation xml:lang="NL">Lokaal Overweg Schakelmateriaal</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLocalControlEquipment">
					<xs:attribute name="levelCrossingRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de bijbehorende overweg.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>LevelCrossing</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SwitchControls">
