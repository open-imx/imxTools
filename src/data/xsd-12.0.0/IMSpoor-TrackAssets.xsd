			<xs:documentation>Een verzameling sleutelvergrendelkastjes</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="KeyLockControl" minOccurs="0" maxOccurs="unbounded"/>
