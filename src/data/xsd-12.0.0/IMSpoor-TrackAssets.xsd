			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:sequence>
						<xs:element ref="Scanner" minOccurs="0"/>
						<xs:element ref="RailContact" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="hasActiveSignaling" type="tTrueFalseEnum" use="required"/>
					<xs:attribute name="monitoringSystemCabinetRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de gerelateerde Systeemkast</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>MonitoringSystemCabinet</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
