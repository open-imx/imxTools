	<xs:element name="Bridge">
		<xs:annotation>
			<xs:documentation>Een brug is een vaste of beweegbare verbinding voor het verkeer, tussen twee punten die gescheiden zijn door een rivier, kanaal, kloof, dal, weg, spoorweg of een ander obstakel.</xs:documentation>
			<xs:documentation xml:lang="NL">Brug</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPolygonTrackAsset">
					<xs:sequence>
						<xs:element ref="BridgeConstructionPart" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="BridgeSignalRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen brugseinen. Een brugsein is een hoofdsein of stopmarkerboard.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Signal</ObjectTypeRef>
									<ObjectTypeRef>StopMarkerBoard</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="BridgeSectionRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar een of meer brugsecties waarin een beweegbaar deel van de brug voorkomt.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>TrackCircuit</ObjectTypeRef>
									<ObjectTypeRef>AxleCounterSection</ObjectTypeRef>
									<ObjectTypeRef>AfSection</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="SignRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen met het bijbehorende bord RS 243 en/of RS 244</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Sign</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="abbreviation" type="tAbbreviationString" use="optional">
						<xs:annotation>
							<xs:documentation>De verkorte naam</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="isMoveable" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of het kunstwerk beweegbaar is.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="carriesTrack" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of het kunstwerk een spoordragende functie heeft.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="material" type="tMaterialTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het materiaal type van het kunstwerk. De lijst van opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="wildlifeCrossing" type="tWildlifeCrossingTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Een indicatie van het soort faunavoorziening waarmee het kunstwerk is uitgevoerd. De lijst van opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="waterWayName" type="tString50" use="required">
						<xs:annotation>
							<xs:documentation>De naam van de waterweg waarmee het kunstwerk kruist.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="hasBridgeWatcherFunction" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of de brug een brugwachter-functie heeft</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="isShippingPriority" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of scheepvaartverkeer voorrang heeft op treinverkeer</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
