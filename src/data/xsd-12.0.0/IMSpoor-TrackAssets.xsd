						<xs:annotation>
							<xs:documentation>Een verwijzing naar de generieke constructietekening (GTD) van het object.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="locationSpecificDesignDocumentId" type="tString100">
						<xs:annotation>
							<xs:documentation>Een verwijzing naar de locatie specifieke constructietekening (LTD) van het object.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LubricationNozzles">
