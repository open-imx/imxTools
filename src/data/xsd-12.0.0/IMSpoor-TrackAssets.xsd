			<xs:documentation>Een verzameling Lokaal wissel Schakelmateriaal</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SwitchControl" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SwitchControl">
		<xs:annotation>
			<xs:documentation>Lokaal Wissel Schakelmateriaal.</xs:documentation>
			<xs:documentation xml:lang="NL">Lokaal Wissel Schakelmateriaal</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLocalControlEquipment">
					<xs:attribute name="switchMechanismRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar de bijbehorende wisselsteller.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>SwitchMechanism</ObjectTypeRef>
							</xs:appinfo>
