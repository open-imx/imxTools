			<xs:sequence>
				<xs:element ref="ControlBooth" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ControlBooth">
		<xs:annotation>
			<xs:documentation>Bedienpost. Een ruimte tbv lokale bediening.</xs:documentation>
			<xs:documentation xml:lang="NL">Bedienpost</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
