	<xs:element name="WorkzoneSwitchCabinets">
		<xs:annotation>
			<xs:documentation>Een verzameling werkzone sleutelkasten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="WorkzoneSwitchCabinet" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WorkzoneSwitchCabinet">
		<xs:annotation>
			<xs:documentation>Een werkzone sleutelkast (ook bekend als werkzone-schakelaar-kast) bevat een of meer werkzone-schakelaars</xs:documentation>
			<xs:documentation xml:lang="NL">Werkzone Sleutelkast</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element name="WorkzoneSwitchRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst PUIC verwijzingen naar werkzoneschakelaars welke zich in de werkzone sleutelkast bevinden</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>WorkzoneSwitch</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="WorkzoneSwitches">
		<xs:annotation>
			<xs:documentation>Een verzameling werkzone-schakelaars</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="WorkzoneSwitch" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WorkzoneSwitch">
		<xs:annotation>
			<xs:documentation>Werkzone-Schakelaar is een schakelaar in een werkzone sleutelkast waarmee één Werkzone genomen kan worden bij aanvang van de werkzaamheden en teruggegeven kan worden aan het eind van de werkzaamheden.</xs:documentation>
			<xs:documentation xml:lang="NL">Werkzone Schakelaar</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:attribute name="workzoneSystemRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>Een PUIC verwijzing naar het bijbehorende werkzone-systeem.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>WorkzoneSystem</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="workzoneSwitchType" type="tWorkzoneSwitchTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type werkzone schakelaar. De lijst van opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ControllerCabinets">
		<xs:annotation>
			<xs:documentation>Een verzameling medewerkerskastjes</xs:documentation>
