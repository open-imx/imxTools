		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tLocalControlEquipment">
					<xs:attribute name="bridgeControlSystemRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar een Bruginstallatie.</xs:documentation>
							<xs:appinfo>
								<ObjectTypeRef>BridgeControlSystem</ObjectTypeRef>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactCheckers">
		<xs:annotation>
			<xs:documentation>Een verzameling contactinrichtingen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ContactChecker" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactChecker">
		<xs:annotation>
			<xs:documentation>Contactinrichting ter controle of een brug of waterkering in de controle ligt en veilig gepasseerd kan worden.</xs:documentation>
			<xs:documentation xml:lang="NL">Contactinrichting</xs:documentation>
		</xs:annotation>
