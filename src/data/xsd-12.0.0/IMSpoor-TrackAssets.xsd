		<xs:annotation>
			<xs:documentation>Een verzameling objecten voor flankbeveiliging</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FlankProtectionSystem" minOccurs="0" maxOccurs="unbounded"/>
